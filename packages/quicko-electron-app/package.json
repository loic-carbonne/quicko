    "@types/uuid": "^3.4.5",
    "command-line-args": "^5.1.1",
    "electron-log": "^4.0.6",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "inquirer-fuzzy-path": "^2.1.0",
    "link-module-alias": "^1.2.0",
    "lodash": "^4.17.15",
    "material-ui": "^0.20.2",
    "node-localstorage": "^2.1.5",
    "node-plop": "^0.19.0",
    "node-storage": "0.0.8",
